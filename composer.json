{
  "name": "bonroyage/advent-of-code-2022",
  "description": "Advent of Code years/2022",
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-gmp": "*",
    "laravel-zero/framework": "^v10.3",
    "nunomaduro/termwind": "^1.14"
  },
  "require-dev": {
    "laravel/pint": "^1.13",
    "mockery/mockery": "^1.4.4",
    "pestphp/pest": "^1.21.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "MMXV\\": "editions/2015/",
      "MMXXI\\": "editions/2021/",
      "MMXXII\\": "editions/2022/",
      "MMXXIII\\": "editions/2023/",
      "MMXXIV\\": "editions/2024/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "application"
  ],
  "scripts": {
    "test": "php application test",
    "pint:check": "vendor/bin/pint --test",
    "pint:fix": "vendor/bin/pint"
  }
}
